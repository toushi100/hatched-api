{
  "name": "nest-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "16.13.0",
    "npm": "9.8.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:create": "npm run typeorm migration:create -- -d src/migrations -n",
    "typeorm:generate": "npm run typeorm migration:generate -- -d src/migrations -n",
    "typeorm:run": "npm run typeorm migration:run",
    "typeorm:revert": "npm run typeorm migration:revert",
    "typeorm:build-run": "npm run build && npm run typeorm:run",
    "typeorm:build-revert": "npm run build && npm run typeorm migration:revert",
    "typeorm:drop": "npm run typeorm schema:drop"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs/common": "^7.5.5",
    "@nestjs/core": "^7.6.18",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/schedule": "^2.2.3",
    "@nestjs/swagger": "^4.5.1",
    "@nestjs/typeorm": "^7.1.5",
    "aws-sdk": "^2.1048.0",
    "bcrypt": "^5.0.1",
    "cache-manager": "^3.6.0",
    "class-transformer": "^0.4.0",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.13.1",
    "cli-color": "^2.0.3",
    "commander": "^8.3.0",
    "compression": "^1.7.4",
    "csurf": "^1.10.0",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^5.1.3",
    "handlebars": "^4.7.7",
    "helmet": "^4.1.0",
    "js-sha512": "^0.8.0",
    "moment": "^2.29.1",
    "mysql": "^2.17.1",
    "nestjs-aws": "git+https://github.com/Elephants-Tech/nestjs-aws.git",
    "nestjs-i18n": "^8.1.6",
    "nodemailer": "^6.7.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.6.2",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.25",
    "uuid": "8.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.4",
    "@nestjs/testing": "^7.6.18",
    "@types/bcrypt": "^3.0.0",
    "@types/cache-manager": "^3.4.2",
    "@types/compression": "^1.7.0",
    "@types/csurf": "^1.9.35",
    "@types/dotenv": "8.2.0",
    "@types/express": "^4.17.7",
    "@types/express-rate-limit": "^5.1.0",
    "@types/helmet": "0.0.48",
    "@types/node": "11.13.4",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.2.0",
    "request": "^2.88.2",
    "supertest": "4.0.2",
    "ts-jest": "^27.0.7",
    "ts-loader": "^7.0.4",
    "ts-node": "8.1.0",
    "tsc-watch": "^4.5.0",
    "tsconfig-paths": "3.8.0",
    "typescript": "^4.4.1-rc"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
